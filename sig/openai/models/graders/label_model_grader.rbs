          input: ::Array[OpenAI::EvalItem],
          labels: ::Array[String],
          model: String,
          name: String,
          passing_labels: ::Array[String],
          type: :label_model
        }

      class LabelModelGrader < OpenAI::Internal::Type::BaseModel
